{
  "name": "order-planet-core",
  "version": "1.0.1",
  "description": "order-planet core server",
  "author": "LeeSooBeen",
  "private": true,
  "license": "UNLICENSED",
  "module": "esnext",
  "main": "dist/main.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build --webpack --webpackPath webpack-hmr.config.js",
    "build:swc": "swc --ignore \"**/*.spec.ts\" --delete-dir-on-start --out-dir dist/src/ src/",
    "build:dev": "nest build ",
    "build:prod": "nest build --webpack --webpackPath webpack-hmr.config.js",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev-backup": "nest start --watch",
    "migrate": "npx prisma migrate dev",
    "start:local": "rimraf dist && nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:dev": "rimraf dist && nest build --webpack --webpackPath webpack-hmr.config.js",
    "start:debug": "nest start --debug --watch",
    "start:prod": "npx prisma db push && rimraf dist && nest build nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "lint": "gts lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "clean": "gts clean",
    "eslint": "eslint \"./src/**/*.ts\" && tsc --noemit",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "husky install",
    "pretest": "npm run compile",
    "posttest": "npm run lint"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.418.0",
    "@aws-sdk/client-ses": "^3.535.0",
    "@aws-sdk/client-sns": "^3.427.0",
    "@aws-sdk/client-sqs": "^3.418.0",
    "@fastify/compress": "^6.4.0",
    "@fastify/csrf-protection": "^6.3.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/multipart": "^7.7.3",
    "@fastify/static": "^6.11.2",
    "@golevelup/nestjs-discovery": "^4.0.0",
    "@google-cloud/translate": "^8.0.2",
    "@nestjs-modules/mailer": "^1.11.2",
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/common": "^10.2.6",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.6",
    "@nestjs/cqrs": "^10.2.5",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/mongoose": "^10.0.1",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-fastify": "^10.2.6",
    "@nestjs/platform-socket.io": "^10.2.6",
    "@nestjs/swagger": "^7.1.12",
    "@nestjs/throttler": "^5.0.0",
    "@nestjs/websockets": "^10.2.6",
    "@prisma/client": "^5.4.1",
    "@prisma/extension-read-replicas": "^0.2.2",
    "@socket.io/redis-adapter": "^8.2.1",
    "@ssut/nestjs-sqs": "^2.2.0",
    "@types/jsdom": "^21.1.6",
    "apple-signin-auth": "^1.7.6",
    "async_hooks": "^1.0.0",
    "axios": "^1.5.0",
    "bcrypt": "^5.1.1",
    "cache-manager": "^4.0.0",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.10",
    "fastify": "^4.23.2",
    "firebase-admin": "^12.0.0",
    "geoip-lite": "^1.4.9",
    "google-auth-library": "^9.6.3",
    "helmet": "^7.0.0",
    "jsdom": "^24.0.0",
    "jsencrypt": "^3.3.2",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "nanoid": "^3.3.4",
    "node-rsa": "^1.1.1",
    "passport-jwt": "^4.0.1",
    "redis": "^4.6.10",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.4",
    "rxjs": "^7.8.1",
    "sharp": "^0.32.6",
    "socket.io": "^4.7.2",
    "typescript": "^5.2.2",
    "uuid": "^9.0.1",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@nestjs/cli": "^10.1.18",
    "@nestjs/testing": "^10.2.7",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.89",
    "@types/bcrypt": "^5.0.0",
    "@types/cache-manager": "^4.0.3",
    "@types/cache-manager-redis-store": "^2.0.2",
    "@types/compression": "^1.7.3",
    "@types/cron": "^2.4.0",
    "@types/crypto-js": "^4.1.2",
    "@types/geoip-lite": "^1.4.4",
    "@types/jest": "29.5.5",
    "@types/lodash": "^4.14.199",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^20.7.0",
    "@types/node-rsa": "^1.1.4",
    "@types/passport-jwt": "^3.0.10",
    "@types/request-ip": "^0.0.38",
    "@types/supertest": "^2.0.14",
    "@types/uuid": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "commitlint-config-git-commit-emoji": "^1.0.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "gts": "^5.0.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^14.0.1",
    "npm": "^10.1.0",
    "prettier": "^3.0.3",
    "prettier-eslint": "^15.0.1",
    "prisma": "^5.4.1",
    "run-script-webpack-plugin": "^0.2.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "swc-loader": "^0.2.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "webpack": "^5.88.2",
    "webpack-node-externals": "^3.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,json,css,scss,md}": [
      "npx prettier --write",
      "echo \"npm run lint --prefix webapp --fix\"",
      "eslint"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "modulePaths": [
      "<rootDir>"
    ]
  }
}
